<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.effect.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>

<VBox style="-fx-background-color: #ffff00;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox prefHeight="100.0" prefWidth="400.0" style="-fx-background-color: #00ff00; -fx-border-color: #00000f; -fx-border-insets: 10; -fx-border-radius: 15;">
         <children>
            <Button fx:id="btn1" mnemonicParsing="false" prefHeight="100.0" prefWidth="100.0" style="-fx-border-insets: 100;" text="Button">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin></Button>
            <Button fx:id="btn2" mnemonicParsing="false" prefHeight="100.0" prefWidth="100.0" style="-fx-background-color: #ff0000;" text="Button" />
         </children>
      </HBox>
      <HBox prefHeight="400.0" prefWidth="200.0" style="-fx-background-color: linear-gradient(to right,#000000,#ffffff);">
         <children>
            <TableView prefHeight="400.0" prefWidth="240.0">
              <columns>
                <TableColumn prefWidth="75.0" text="C1" />
                <TableColumn prefWidth="75.0" text="C2" />
              </columns>
            </TableView>
         </children>
      </HBox>
      <HBox prefHeight="100.0" prefWidth="200.0" style="-fx-background-image: url('file:ppp/회원.png');">
         <children>
            <TextField />
            <Button mnemonicParsing="false" style="-fx-background-color: #ff0000;" text="Button" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="30.0" left="30.0" right="30.0" top="10.0" />
   </padding>
</VBox>
