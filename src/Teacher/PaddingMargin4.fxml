      </HBox>
      <HBox id="hbox2" prefHeight="400.0" prefWidth="200.0" >
         <children>
            <TableView prefHeight="400.0" prefWidth="240.0">
              <columns>
                <TableColumn prefWidth="75.0" text="C1" />
                <TableColumn prefWidth="75.0" text="C2" />
              </columns>
            </TableView>
         </children>
      </HBox>
      <HBox id="hbox3" prefHeight="100.0" prefWidth="200.0" >
         <children>
            <TextField />
